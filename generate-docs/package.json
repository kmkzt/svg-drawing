{
  "name": "svg-drawing-generate-docs",
  "private": true,
  "version": "5.0.1-beta.0",
  "scripts": {
    "build": "typedoc  --entryPointStrategy packages ../ --out ../docs/packages --excludePrivate --plugin typedoc-plugin-markdown --hideInPageTOC --hideBreadcrumbs --filenameSeparator \"/\""
  },
  "dependencies": {
    "@svg-drawing/animation": "5.0.1-beta.0",
    "@svg-drawing/core": "5.0.1-beta.0",
    "@svg-drawing/img-trace": "5.0.1-beta.0",
    "@svg-drawing/react": "5.0.1-beta.0"
  },
  "devDependencies": {
    "typedoc": "0.22.17",
    "typedoc-plugin-markdown": "3.12.1"
  }
}
